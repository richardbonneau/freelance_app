rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      function isSignedIn(){
        return request.auth.uid != null;
      }

      allow read, write: if false

      // allow read, write: if isSignedIn() && request.auth.uid == ressource.data.uid
    }
  }
}